{
  "name": "sf-digital-signature-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist && yarn format",
    "build": "yarn prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "reset": "rimraf node_modules && yarn",
    "start:dev": "yarn prebuild && cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/microservices": "^6.11.7",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^4.2.6",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.5",
    "aws-sdk": "^2.824.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "0.2.3",
    "class-validator": "^0.9.1",
    "config": "^3.2.5",
    "dayjs": "^1.10.4",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.1",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/schematics": "^6.9.3",
    "@nestjs/testing": "^6.11.5",
    "@types/jest": "^25.1.1",
    "@types/supertest": "^2.0.8",
    "cross-env": "^7.0.0",
    "jest": "^24.9.0",
    "mongoose": "^5.12.3",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
